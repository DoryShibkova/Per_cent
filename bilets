a = int(input())
b = []

for i in range (1, a+1):
    
    b[i] = int(input())
    
    if b[i] >= 18 and b[i] < 25:
        
        c = b[i] * 990
        
    elif b >= 25:
        
        n = b * 1390
        
    s = sum(n, c)
    
    if a > 3:
        
        s = s - 10%
    else:
        
        return (s)

print(s)
